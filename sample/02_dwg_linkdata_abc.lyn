// Dynasty Warriors Gundam linkdata files

main {
    intb numEntries 0x4; // intb is big-endian int (s32be)
    file[numEntries] entries 0x10;
}

file 0x8 {
    intb offsetChunk 0;
    intb length 0x4;
    value offset (offsetChunk * 0x800) - $a; // $a is a replacement
    output data [offset, length: length] $i+".dat";
}