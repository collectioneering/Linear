// Dynasty Warriors Gundam linkdata files

main {
    intb numEntries 0x4;
    file[numEntries] entries 0x10;
}

file 0x8 {
    intb offsetChunk 0;
    intb length 0x4;
    value offset (offsetChunk * 0x800) - ($i * 8) - 0x10; // TODO support $a replacement for absolute offset
    output data [offset, length: length] $i+".dat";
}