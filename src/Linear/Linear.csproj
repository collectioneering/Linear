<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8</LangVersion>
    <Authors>Lucina</Authors>
    <Description>Linear file layout spec</Description>
    <Copyright>Copyright Â©2020 Lucina</Copyright>
    <PackageProjectUrl>https://github.com/Lucina/Linear</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Lucina/Linear</RepositoryUrl>
    <Company>Lucina</Company>
    <AssemblyVersion>1.0.0</AssemblyVersion>
    <PackOnBuild>true</PackOnBuild>
    <Owners>Lucina</Owners>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageVersion>0.1.0</PackageVersion>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>3021;1584;1658</NoWarn>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Antlr4.Runtime.Standard" Version="4.8.0" />
    <PackageReference Include="Esper" Version="0.3.2" />
    <PackageReference Include="System.Memory" Version="4.5.3" />
  </ItemGroup>

</Project>
